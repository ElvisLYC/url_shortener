<table>
  <tr>
    <th>long_url</th>
    <th>short_url</th>
  </tr>

  <% @urls.each do |url| %>
    <tr>
      <a href="#">
        <td><%= link_to "long url", url_path(url.id) %></td>
      </a>

        <td>
            <a href=<%= url.long_url %>>
              <%= url.short_url %>
          </a>
        </td>

    </tr>
    <% end %>
</table>
